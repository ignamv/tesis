	file = {IEEE Xplore Abstract Record:/home/ignamv/.mozilla/firefox/21liga2a.default/zotero/storage/PC6U4IHE/abs_all.html:text/html;Tarr et al_2004_A sensitive, temperature-compensated, zero-bias floating gate MOSFET dosimeter.pdf:/home/ignamv/.mozilla/firefox/21liga2a.default/zotero/storage/EN58NC26/Tarr et al_2004_A sensitive, temperature-compensated, zero-bias floating gate MOSFET dosimeter.pdf:application/pdf}
