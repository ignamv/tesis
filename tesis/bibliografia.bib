@article{cellere_radiation_2001,
	title = {Radiation effects on floating-gate memory cells},
	volume = {48},
	issn = {0018-9499},
	doi = {10.1109/23.983199},
	abstract = {We have addressed the problem of threshold voltage ({VTH}) variation in flash memory cells after heavy-ion irradiation by using specially designed array structures and test instruments. After irradiation, low {VTH} tails appear in {VTH} distributions, growing with ion linear energy transfer ({LET}) and fluence. In particular, high {LET} ions, such as iodine used in this paper, can produce a bit flip. Since the existing models cannot account for large charge losses from the floating gate, we propose a new mechanism, based on the excess of positive charge produced by a single ion, temporarily lowering the tunnel oxide barrier (positive charge assisted leakage current) and enhancing the tunneling current. This mechanism fully explains the experimental data we present},
	pages = {2222--2228},
	number = {6},
	journaltitle = {{IEEE} Transactions on Nuclear Science},
	author = {Cellere, G. and Pellati, P. and Chimenton, A. and Wyss, J. and Modelli, A. and Larcher, L. and Paccagnella, A.},
	date = {2001-12},
	keywords = {\_tablet, array structure, bit flip, charge loss, Energy exchange, flash memories, Flash memory cells, floating-gate flash memory cell, heavy ion irradiation, I, Instruments, ion beam effects, Leakage current, leakage currents, linear energy transfer, Nonvolatile memory, positive charge assisted leakage current, Probability distribution, Radiation Effects, Testing, test instrument, Threshold voltage, tunneling, tunneling current, tunnelling, tunnel oxide barrier},
	file = {Cellere et al_2001_Radiation effects on floating-gate memory cells.pdf:/home/ignacio/.mozilla/firefox/akolirhp.default/zotero/storage/9BMN5E2J/Cellere et al_2001_Radiation effects on floating-gate memory cells.pdf:application/pdf}
